{
  "name": "@cumulus/api-client",
  "version": "1.19.0",
  "description": "API client for working with the Cumulus archive API",
  "keywords": [
    "GIBS",
    "CUMULUS",
    "NASA"
  ],
  "engines": {
    "node": ">=10.16.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/nasa/cumulus/tree/master/packages/api-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa/cumulus"
  },
  "scripts": {
    "build": "true",
    "lint": "eslint .",
    "test": "ava",
    "test-coverage": "nyc ava",
    "test-ci": "npm run lint"
  },
  "ava": {
    "files": [
      "tests/**"
    ],
    "verbose": true
  },
  "nyc": {
    "exclude": [
      "tests",
      "test"
    ]
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "@cumulus/aws-client": "1.19.0",
    "@cumulus/common": "1.19.0",
    "@cumulus/logger": "1.19.0",
    "base-64": "0.1.0",
    "form-data": "^3.0.0",
    "got": "^9.2.2",
    "jsonwebtoken": "^8.4.0",
    "normalize-url": "5.0.0",
    "p-limit": "^2.0.0",
    "parseurl": "^1.3.3"
  },
  "devDependencies": {
    "ava": "^2.1.0",
    "lodash.ismatch": "^4.4.0",
    "p-retry": "4.2.0",
    "rewire": "^4.0.1"
  }
}
